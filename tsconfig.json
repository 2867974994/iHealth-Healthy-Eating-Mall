{
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "ES2020",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictPropertyInitialization": true,
    "lib": ["ES2020"],
    "typeRoots": [
      "./typings"
    ]
  },
  "include": [
    "./**/*.ts"
, "miniprogram/pages/about/about.js", "miniprogram/pages/food/food.js", "miniprogram/pages/myCart/myCart.js", "miniprogram/pages/index/index.js", "miniprogram/pages/indexDetail/indexDetail.js", "miniprogram/pages/search/search.js", "miniprogram/pages/selectCity/selectCity.js", "miniprogram/utils/util.tx", "miniprogram/templates/foodList/foodList.js", "miniprogram/pages/productType/productType.js", "miniprogram/app.js", "miniprogram/pages/productDetail/productDetail.js"  ],
  "exclude": [
    "node_modules"
  ]
}
